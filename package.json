{
  "name": "whatsapp-chat-generator",
  "version": "1.0.0",
  "description": "WhatsApp离线版聊天记录生成器",
  "main": "index.js",
  "scripts": {
    "dev": "node node_modules/vite/bin/vite.js",
    "postinstall": "chmod +x node_modules/.bin/* || true",
    "prebuild": "chmod +x node_modules/.bin/* || true",
    "build": "node node_modules/vite/bin/vite.js build",
    "build:fallback": "npx vite build",
    "build:safe": "node build.js",
    "preview": "node node_modules/vite/bin/vite.js preview",
    "build:ios": "npm run build && npx cap sync ios",
    "open:ios": "npx cap open ios",
    "sync:ios": "npx cap sync ios",
    "ios:dev": "npm run build:ios && npx cap open ios",
    "ios:build": "npm run build:ios"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.2",
    "@capacitor/core": "^7.4.2",
    "@capacitor/ios": "^7.4.2",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
