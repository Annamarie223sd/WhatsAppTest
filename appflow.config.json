{
  "build": {
    "ios": {
      "commands": [
        "node node_modules/vite/bin/vite.js build",
        "npx cap sync ios"
      ],
      "environment": {
        "NODE_ENV": "production"
      }
    }
  },
  "platforms": {
    "ios": {
      "buildType": "release",
      "signing": {
        "certificateType": "development"
      }
    }
  },
  "environments": {
    "production": {
      "NODE_ENV": "production",
      "IONIC_ENV": "prod"
    }
  }
}
